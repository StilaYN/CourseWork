<Window x:Class="CourseWork.EnterLoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseWork"
        xmlns:modelview="clr-namespace:CourseWork.ViewModel"
        d:DataContext="{d:DesignInstance Type=modelview:EnterLoginVM}"
        mc:Ignorable="d"
        Title="EnterLogin" Height="240" Width="300">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,41,0,0" TextWrapping="Wrap" Text="{Binding FieldName}" TextAlignment="Center" VerticalAlignment="Top" Width="164" FontFamily="Bahnschrift" FontSize="14"/>
        <TextBox HorizontalAlignment="Left" Margin="58,75,0,0" TextWrapping="Wrap" Text="{Binding Field}" TextAlignment="Center" VerticalAlignment="Top" Width="160" FontSize="14"/>
        <Button Content="Done" Command="{Binding Done}" HorizontalAlignment="Center" Margin="0,124,0,0" VerticalAlignment="Top" Width="72" Height="32" FontFamily="Bahnschrift" FontSize="14"/>
        <Button Content="end" HorizontalAlignment="Left" Margin="223,75,0,0" VerticalAlignment="Top" Height="21" FontFamily="Bahnschrift" FontSize="14"/>

    </Grid>
</Window>